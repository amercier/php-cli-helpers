{
  "name": "amercier/cli-helpers",
  "description": "Utility classes to write PHP command-line scripts",
  "type": "library",
  "keywords": [
    "cli",
    "helpers",
    "command",
    "line",
    "command-line",
    "utility",
    "utilities"
  ],
  "homepage": "https://github.com/amercier/php-cli-helpers",
  "license": "MIT",
  "autoload": {
    "psr-0": {
      "Cli": "src/"
    }
  },
  "include-path": [
    "src/"
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.php.net"
    }
  ],
  "require": {
    "php": ">=5.3.2"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "phpunit/phpunit": "4.*",
    "pear-pear.php.net/PHP_CodeSniffer": ">=1.4.5"
  }
}
