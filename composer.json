{
  "name": "amercier/cli-helpers",
  "description": "Utility classes to write PHP command-line scripts",
  "type": "library",
  "keywords": [
    "cli",
    "helpers",
    "command",
    "line",
    "command-line",
    "utility",
    "utilities"
  ],
  "homepage": "https://github.com/amercier/php-cli-helpers",
  "license": "MIT",
  "require": {
    "php": ">=5.3.2"
  },
  "autoload": {
    "psr-0": {
      "VMware_": "library/"
    }
  },
  "include-path": [
    "library/"
  ],
  "repositories": [
    {
      "type": "pear",
      "url": "http://pear.php.net"
    }
  ],
  "require": {
    "vmware/vcloud-sdk-patched": "512.0.0",
    "pear-pear/HTTP_Request2": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "3.*"
  }
}
